{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "max-len": "off",
    "no-use-before-define": "off",
    "no-plusplus": "off",
    "no-tabs": "off",
    "no-mixed-spaces-and-tabs": "off",
    "no-alert": "off"
  }
}
